{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "players",
  "info": {
    "name": "Player"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "room": {
      "model": "room"
    },
    "socket": {
      "type": "uid"
    },
    "cards": {
      "type": "json"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "black",
        "blue",
        "green",
        "white",
        "yellow",
        "red"
      ]
    }
  }
}
